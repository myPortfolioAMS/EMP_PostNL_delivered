{
  "name": "emp",
  "version": "0.1.0",
  "bin": {
    "emp": "bin/emp_poc_stack.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm-run-all --parallel build:*",
    "build:event-producer": "esbuild lambda/event-producer/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer",
    "build:event-producer-first-sorting": "esbuild lambda/event-producer-first-sorting/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer-first-sorting",
    "build:event-producer-cross-docking": "esbuild lambda/event-producer-cross-docking/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer-cross-docking",
    "build:event-producer-second-sorting": "esbuild lambda/event-producer-second-sorting/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer-second-sorting",
    "build:event-producer-distribution": "esbuild lambda/event-producer-distribution/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer-distribution",
    "build:event-producer-final-consumer": "esbuild lambda/event-producer-final-consumer/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-producer-final-consumer",
    "build:parcel-event": "esbuild lambda/event-consumer/parcel-event/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-consumer/parcel-event",
    "build:parcel-event-from-dynamodb": "esbuild lambda/event-consumer/from-dynamodb-parcel-event/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-consumer/from-dynamodb-parcel-event",
    "build:track-trace-event": "esbuild lambda/event-consumer/track-trace-event/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-consumer/track-trace-event",
    "build:digital-label-event": "esbuild lambda/event-consumer/digital-label-event/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-consumer/digital-label-event",
    "build:parcel-event-to-dynamodb": "esbuild lambda/event-consumer/to-dynamodb-parcel-event/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-consumer/to-dynamodb-parcel-event",
    "build:register-schema": "esbuild lambda/register-schema/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/register-schema",
    "build:event-management": "esbuild lambda/event-management/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-management",
    "build:event-monitor": "esbuild lambda/event-monitor/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/event-monitor",
    "build:auth-register": "esbuild lambda/auth/register/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/auth/register",
    "build:auth-login": "esbuild lambda/auth/login/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/auth/login",
    "build:event-master-plan": "esbuild lambda/master-plan/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/master-plan",
    "build:event-priority-master-plan": "esbuild lambda/priority-master-plan/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/priority-master-plan",
    "build:listener-execution-plan": "esbuild lambda/listener-execution-plan/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/listener-execution-plan",
    "build:keep-them-warm": "esbuild lambda/keepThemWarm/index.ts --bundle --platform=node --target=node20 --external:aws-sdk --outdir=dist/lambda/keepThemWarm",
    "deploy": "npm run clean && npm run build && cdk deploy --all",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@aws-cdk/aws-cloudwatch-actions": "^1.204.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.750.0",
    "@aws-sdk/client-dynamodb": "^3.744.0",
    "@aws-sdk/client-eventbridge": "^3.744.0",
    "@aws-sdk/client-lambda": "^3.744.0",
    "@aws-sdk/client-s3": "^3.744.0",
    "@aws-sdk/client-schemas": "^3.744.0",
    "@aws-sdk/client-sns": "^3.750.0",
    "@aws-sdk/client-sqs": "^3.744.0",
    "@aws-sdk/lib-dynamodb": "^3.751.0",
    "aws-embedded-metrics": "^4.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "aws-cdk": "2.178.1",
    "aws-cdk-lib": "^2.178.2",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  }
}
